    {
      "title": "Certbot: Obtain certificate (Nginx)",
      "body": "sudo certbot --nginx -d {{domain}}",
      "description": "Get Let's Encrypt SSL certificate and auto-configure Nginx",
      "tags": ["certbot", "ssl", "letsencrypt", "nginx"],
      "language": "bash",
      "created_at": "2025-12-21T10:00:00.000Z",
      "updated_at": "2025-12-21T10:00:00.000Z"
    },
    {
      "title": "Certbot: Obtain certificate (Apache)",
      "body": "sudo certbot --apache -d {{domain}}",
      "description": "Get Let's Encrypt SSL certificate and auto-configure Apache",
      "tags": ["certbot", "ssl", "letsencrypt", "apache"],
      "language": "bash",
      "created_at": "2025-12-21T10:00:00.000Z",

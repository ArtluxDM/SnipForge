    },
    {
      "title": "Curl: POST form data",
      "body": "curl -X POST {{url}} \\\n  -F \"{{field name}}={{value}}\" \\\n  -F \"file=@{{filepath}}\"",
      "description": "POST multipart form data with file upload",
      "tags": ["curl", "http", "post", "form", "upload"],
      "language": "bash",

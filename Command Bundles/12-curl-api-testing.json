      "updated_at": "2025-12-21T10:00:00.000Z"
    },
    {
      "title": "Curl: Test API response time",
      "body": "curl -w \"\\nTime: %{time_total}s\\n\" -o /dev/null -s {{url}}",
      "description": "Measure API response time without showing response body",
      "tags": ["curl", "http", "performance", "timing"],
      "language": "bash",

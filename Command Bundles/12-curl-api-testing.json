  "commands": [
    {
      "title": "Curl: GET request",
      "body": "curl {{url}}",
      "description": "Simple GET request to URL",
      "tags": ["curl", "http", "api"],
      "language": "bash",
      "created_at": "2025-12-21T10:00:00.000Z",
      "updated_at": "2025-12-21T10:00:00.000Z"
    },
    {
      "title": "Curl: GET with headers",
      "body": "curl -H \"{{header name}}: {{header value}}\" {{url}}",
      "description": "GET request with custom header",
      "tags": ["curl", "http", "api", "headers"],
      "language": "bash",
      "created_at": "2025-12-21T10:00:00.000Z",
      "updated_at": "2025-12-21T10:00:00.000Z"
    },
    {
      "title": "Curl: POST JSON data",
      "body": "curl -X POST {{url}} \\\n  -H \"Content-Type: application/json\" \\\n  -d '{{json data}}'",
      "description": "POST JSON data to API endpoint",
      "tags": ["curl", "http", "api", "post", "json"],
      "language": "bash",
      "created_at": "2025-12-21T10:00:00.000Z",
      "updated_at": "2025-12-21T10:00:00.000Z"
    },
    {
      "title": "Curl: POST with Bearer token",
      "body": "curl -X POST {{url}} \\\n  -H \"Authorization: Bearer {{token}}\" \\\n  -H \"Content-Type: application/json\" \\\n  -d '{{json data}}'",
      "description": "POST with JWT/Bearer authentication",
      "tags": ["curl", "http", "api", "auth", "jwt"],
      "language": "bash",
      "created_at": "2025-12-21T10:00:00.000Z",
      "updated_at": "2025-12-21T10:00:00.000Z"
    },
    {
      "title": "Curl: PUT request",
      "body": "curl -X PUT {{url}} \\\n  -H \"Content-Type: application/json\" \\\n  -d '{{json data}}'",
      "description": "PUT request to update resource",
      "tags": ["curl", "http", "api", "put"],
      "language": "bash",
      "created_at": "2025-12-21T10:00:00.000Z",
      "updated_at": "2025-12-21T10:00:00.000Z"
    },
    {
      "title": "Curl: DELETE request",
      "body": "curl -X DELETE {{url}} \\\n  -H \"Authorization: Bearer {{token}}\"",
      "description": "DELETE request to remove resource",
      "tags": ["curl", "http", "api", "delete"],
      "language": "bash",
      "created_at": "2025-12-21T10:00:00.000Z",
      "updated_at": "2025-12-21T10:00:00.000Z"
    },
    {
      "title": "Curl: Basic authentication",
      "body": "curl -u {{username}}:{{password}} {{url}}",
      "description": "GET request with basic auth credentials",
      "tags": ["curl", "http", "api", "auth"],
      "language": "bash",
      "created_at": "2025-12-21T10:00:00.000Z",
      "updated_at": "2025-12-21T10:00:00.000Z"
    },
    {
